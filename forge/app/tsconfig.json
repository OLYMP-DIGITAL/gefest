{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    // "jsx": "react",
    "types": ["node", "react-native", "jest", "react"],
    "strict": true,
    "baseUrl": "./src",
    "forceConsistentCasingInFileNames": true,

    "paths": {
      "@core/*": ["core/*"],
      "@assets/*": ["assets/*"]
    },
  },

  "exclude": ["node_modules"]
}